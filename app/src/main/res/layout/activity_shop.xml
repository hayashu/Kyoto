<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Shop">

    <ImageButton
        android:id="@+id/gionnkoishi"
        android:layout_width="200dp"
        android:layout_height="100dp"
        android:layout_marginEnd="12dp"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/saryousuisenn"
        app:layout_constraintTop_toBottomOf="@+id/marukyukoyamaenn"
        app:layout_constraintVertical_bias="0.06"
        app:srcCompat="@drawable/gionn" />

    <ImageButton
        android:id="@+id/itoen"
        android:layout_width="200dp"
        android:layout_height="100dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:scaleType="fitCenter"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ito" />

    <ImageButton
        android:id="@+id/nakamuratokichi"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="53dp"
        android:scaleType="fitCenter"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/itoen"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/tokichi" />

    <ImageButton
        android:id="@+id/marukyukoyamaenn"
        android:layout_width="200dp"
        android:layout_height="100dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="24dp"
        android:scaleType="fitCenter"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/itoen"
        app:srcCompat="@drawable/marukyu" />

    <ImageButton
        android:id="@+id/kyohayashi"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="24dp"
        android:scaleType="fitCenter"
        app:layout_constraintStart_toEndOf="@+id/marukyukoyamaenn"
        app:layout_constraintTop_toBottomOf="@+id/nakamuratokichi"
        app:srcCompat="@drawable/hayashi" />

    <ImageButton
        android:id="@+id/saryousuisenn"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="32dp"
        android:scaleType="fitCenter"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/kyohayashi"
        app:srcCompat="@drawable/suisenn" />

    <ImageButton
        android:id="@+id/nishikiichiha"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="32dp"
        android:scaleType="fitCenter"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/saryousuisenn"
        app:srcCompat="@drawable/nishikiichiha" />

    <ImageButton
        android:id="@+id/hukujyuennn"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="7dp"
        android:layout_marginBottom="24dp"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/nishikiichiha"
        app:layout_constraintTop_toBottomOf="@+id/gionnkoishi"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/huku" />

    <ImageButton
        android:id="@+id/nagatake"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="7dp"
        android:scaleType="fitCenter"
        app:layout_constraintEnd_toStartOf="@+id/hukujyuennn"
        app:layout_constraintTop_toBottomOf="@+id/gionnkoishi"
        app:srcCompat="@drawable/nagatake" />

    <Button
        android:id="@+id/morecheck"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp"
        android:text="もっと見る"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textSize="36sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>